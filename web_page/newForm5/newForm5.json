    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.cuenta ){",
        "\treturn {",
        "\t\t//map cuenta variable to expected task contract input",
        "\t\tcuentaInput: {",
        "\t\t\tnombreDoc: $data.cuenta.nombreDoc !== undefined ? $data.cuenta.nombreDoc : null,",
        "\t\t\ttipoDoc: $data.cuenta.tipoDoc !== undefined ? $data.cuenta.tipoDoc : null,",
        "\t\t\tfecha: $data.cuenta.fecha !== undefined ? $data.cuenta.fecha : null,",
        "\t\t\tdescripcion: $data.cuenta.descripcion !== undefined ? $data.cuenta.descripcion : null,",
        "\t\t\tarchivo: $data.cuenta.archivo !== undefined ? $data.cuenta.archivo : null,",
        "\t\t\tencargado: $data.cuenta.encargado !== undefined ? $data.cuenta.encargado : null",
        "\t\t}",
        "\t}",
        "}"
      ],
