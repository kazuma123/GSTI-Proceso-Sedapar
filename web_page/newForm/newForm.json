      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.notasDeEntrada ){",
        "\treturn {",
        "\t\t//map notasDeEntrada variable to expected task contract input",
        "\t\tnotasDeEntradaInput: {",
        "\t\t\tapellido: $data.notasDeEntrada.apellido !== undefined ? $data.notasDeEntrada.apellido : null,",
        "\t\t\tnombre: $data.notasDeEntrada.nombre !== undefined ? $data.notasDeEntrada.nombre : null,",
        "\t\t\tdni: $data.notasDeEntrada.dni !== undefined ? $data.notasDeEntrada.dni : null,",
        "\t\t\tarchivo: $data.notasDeEntrada.archivo !== undefined ? $data.notasDeEntrada.archivo : null,",
        "\t\t\tcurso: $data.notasDeEntrada.curso !== undefined ? $data.notasDeEntrada.curso : null",
        "\t\t},",
        "\t\tdocumentoNotasEntradaDocumentInput: $data.formInput.documentoNotasEntradaDocumentInput",
        "\t}",
        "}"
      ],
