    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.alumno ){",
        "\treturn {",
        "\t\t//map alumno variable to expected task contract input",
        "\t\talumnoInput: {",
        "\t\t\tapellido: $data.alumno.apellido !== undefined ? $data.alumno.apellido : null,",
        "\t\t\tnombre: $data.alumno.nombre !== undefined ? $data.alumno.nombre : null,",
        "\t\t\tdni: $data.alumno.dni !== undefined ? $data.alumno.dni : null,",
        "\t\t\tarchivo: $data.alumno.archivo !== undefined ? $data.alumno.archivo : null,",
        "\t\t\tcurso: $data.alumno.curso !== undefined ? $data.alumno.curso : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
